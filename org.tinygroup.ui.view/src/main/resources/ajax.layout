${pageContent}
<script type="text/javascript">

$(function(){
	var a = location.hash.replace(/^#/, "");
	if(!a){ a="#link("/page/index.pagelet")";}
	TinyAjaxloadContent(a, $("#tinypagecontent"));
	function setPageInfo(e){
		window.location.hash=$(e).attr("href");
		var currenttxt=$(e).find('span:first').html();
		$(document).attr("title",currenttxt+"--"+"TinyUiEnterprise展示");
		$('.postion-nav .active').html(currenttxt);
		TinyAjaxloadContent($(e).attr("href"), $("#tinypagecontent"));
	}
	$("body").on("click","a:not([data-toggle=modal])[href]",function(){
		if($(this).attr("href").indexOf(".page")=="-1") return;
		setPageInfo(this);
		return false;
	});
});


function TinyAjaxloadContent(a, b) {
    var time="";
    a=a.replace(/\.pagelet/,".page").replace(/\.page/,".pagelet");
    $.ajax({
        type: "GET",
        url: a,
        dataType: "html",
        beforeSend: function() {
		b[0] == $("#tinypagecontent")[0] && ($("html").animate({
			scrollTop: 0
		},
		"fast"));
		time=setTimeout(function(){
			$("#tinypagecontent").append('<h1 class="ajax-loading-animation tinyLoadingContent" ><i class="fa fa-cog fa-spin"></i> 加载...</h1>');
		},150);
        },
        success: function(a) {
	   if(time) clearTimeout(time);
	   b.html(a);

	   SyntaxHighlighter.highlight();
        },
        error: function() {
            b.html('<h4 class="ajax-loading-error"><i class="fa fa-warning txt-color-orangeDark"></i> Error 404! Page not found.</h4>')
        },
        cache:false
    })
}

</script>



#@dataTable({"id":"example"})
<thead>
    <tr>
        <th class="unsortable"><input type="checkbox" name="allck" value=""></th>
        <th>名称</th>
        <th>城市</th>
        <th>办公室</th>
        <th>年龄</th>
        <th>就职日期</th>
        <th>薪资</th>
        <th class="unsortable">操作</th>
    </tr>
</thead>
<tbody>
#for(var:[1..300])
    <tr>
        <td><input type="checkbox" value="${var}" name=""></td>
        <td>习近平${var}</td>
        <td>北京</td>
        <td>中南海</td>
        <td>67</td>
        <td>2013/03/25</td>
        <td>￥320,800</td>
        <td>
            <a href="#">#faIcon("edit")编辑</a>
            <a href="#">#faIcon("remove")删除</a>
        </td>
    </tr>
#end
</tbody>
#end